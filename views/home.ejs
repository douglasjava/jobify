       
    <%- include('header') %>
        
        <h2 class="style-title">Open roles:</h2>
        <% categorias.forEach( categoria => { %>        
            <div>
                <h3 class="font-sub-title"> <%- categoria.categoria %> </h3>
                <ul class="marcador-list">
                    <% categoria.vagas.forEach( vaga => { %>
                        <li class="marcador-list-itens"> 
                            <a href="/vaga/<%- vaga.id %>">     <%- vaga.titulo %>       </a>
                        </li>
                    <% }) %>
                </ul>
            </div>
        <% }) %>
    
    <%- include('footer') %>


